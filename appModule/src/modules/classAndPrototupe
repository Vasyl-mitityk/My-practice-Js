class Comment {
  constructor(text) {
    this.text = text;
    this.votesQty = 0;
  }

  upvote() {
    this.votesQty += 1;
  }
}

const firstComment = new Comment('First comment');
const seconComment = new Comment('Second comment');
const thirdComment = new Comment('Third commend');

console.log(firstComment);

